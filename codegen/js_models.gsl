  new cpp_call_argument to my.parent
    cpp_call_argument.name = js_cpp_variable_name(my.source_argument.name)
    return cpp_call_argument
  endnew
endfunction

function js_append_cpp_size_argument_model(parent, source_argument)
  new cpp_size_argument to my.parent
    cpp_size_argument.name = js_cpp_variable_name(my.source_argument.name)
    return cpp_size_argument
  endnew
endfunction

function js_append_cpp_data_argument_model(parent, source_argument)
