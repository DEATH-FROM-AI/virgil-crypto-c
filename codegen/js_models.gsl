function js_append_resolved_argument(parent, source_argument, cpp_class, wrapper)
  if js_value_is_size(my.source_argument)
    js_append_cpp_size_argument_model(my.parent, my.source_argument)
  elsif js_value_is_data(my.source_argument)
    js_append_cpp_data_argument_model(my.parent, my.source_argument)
  elsif js_value_is_buffer(my.source_argument)
    js_append_cpp_buffer_argument_model(my.parent, my.source_argument, my.cpp_class, my.wrapper)
  elsif js_value_is_class_module(my.source_argument, my.wrapper)
    js_append_cpp_context_argument_model(my.parent, my.source_argument)
  elsif js_value_is_error_context(my.source_argument)
    js_append_cpp_error_context_argument_model(my.parent, my.source_argument, my.wrapper)
  elsif js_value_is_enum_module(my.source_argument, my.wrapper)
    js_append_cpp_enum_argument_model(my.parent, my.source_argument)
  elsif js_value_is_interface_module(my.source_argument, my.wrapper)
    js_append_cpp_interface_argument_model(my.parent, my.source_argument)
  elsif js_value_is_char(my.source_argument)
    js_append_cpp_char_argument_model(my.parent, my.source_argument)
  elsif js_value_is_byte(my.source_argument)
    js_append_cpp_byte_argument_model(my.parent, my.source_argument)
  elsif js_value_is_integer(my.source_argument)
    js_append_cpp_integer_argument_model(my.parent, my.source_argument)
  elsif js_value_is_unsigned(my.source_argument)
    js_append_cpp_unsigned_argument_model(my.parent, my.source_argument)
  elsif js_value_is_boolean(my.source_argument)
    js_append_cpp_boolean_argument_model(my.parent, my.source_argument)
  else
    echo_fatal("Unable to resolve argument", my)
  endif
endfunction

