    return cpp_data_argument
  endnew
endfunction

function js_append_cpp_call_argument_model_from_source_proxy(parent, source_proxy)
  new cpp_call_argument to my.parent
    if defined(my.source_proxy.argument)
      cpp_call_argument.name = js_cpp_variable_name(my.source_proxy.argument)
    endif
    if defined(my.source_proxy.constant)
      cpp_call_argument.constant = my.source_proxy.constant
    endif
    if defined(my.source_proxy.cast)
      cpp_call_argument.cast = my.source_proxy.cast
    endif
    return cpp_call_argument
  endnew
endfunction

function js_append_cpp_buffer_size_model(parent, name, source_length, cpp_class, wrapper)
  new cpp_buffer_size to my.parent
    if defined(my.source_length.argument)
      cpp_buffer_size.name = my.source_length.argument
    else
      cpp_buffer_size.name = js_cpp_size_name(my.name)
      if defined(my.source_length.constant)
        cpp_buffer_size.type = global.js_cpp_size_type_constant
        cpp_buffer_size.constant = "$(my.wrapper.prefix:no)_$(my.cpp_class.c_name:no)_$(my.source_length.constant:c,upper)"
      elsif defined(my.source_length.method)
        cpp_buffer_size.type = global.js_cpp_size_type_method
        cpp_buffer_size.method = "$(my.wrapper.prefix:no)_$(my.cpp_class.c_name:no)_$(my.source_length.method:c)"
        for my.source_length.proxy
          js_append_cpp_call_argument_model_from_source_proxy(cpp_buffer_size, proxy)
        endfor
      else
        echo_fatal("'source_length.constant' or 'source_length.method' should be defined", my)
      endif
    endif
    return cpp_buffer_size
  endnew
endfunction

function js_append_cpp_buffer_argument_model(parent, source_argument, cpp_class, wrapper)
  new cpp_buffer_argument to my.parent
    cpp_buffer_argument.name = js_cpp_variable_name(my.source_argument.name)
    if defined(my.source_argument->length)
      js_append_cpp_buffer_size_model(cpp_buffer_argument, cpp_buffer_argument.name, my.source_argument->length, my.cpp_class, my.wrapper)
    else
      echo_fatal("'source_argument->length' should be defined", my)
    endif
    return cpp_buffer_argument
  endnew
endfunction

