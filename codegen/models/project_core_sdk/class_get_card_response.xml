<class name="get card response">
    Represent response from CardClient's getCard function with "raw card" and whether or not card is outdated.


    <property name="raw card" class="raw card"/>
    <property name="is outdated" type="boolean"/>


    <constructor name="with">
        Creare a fully defined object.

        <argument name="raw card" class="raw card"/>
        <argument name="is outdated" type="boolean"/>
    </constructor>


    <constructor name="with disown" visibility="private">
        Creare a fully defined object.

        <argument name="raw card" class="raw card" access="disown"/>
        <argument name="is outdated" type="boolean"/>
    </constructor>


    <method name="raw card" is_const="1">
        Return raw representation of Virgil Card.

        <return class="raw card"/>
    </method>


    <method name="is outdated" is_const="1">
        Return true if Virgil Card is outdated, false otherwise.

        <return type="boolean"/>
    </method>
</class>
