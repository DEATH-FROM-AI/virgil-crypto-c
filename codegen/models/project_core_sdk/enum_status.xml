<enum name="status" definition="public">
    Defines the library status codes.

    <constant name="success" value="0">
        No errors was occurred.
    </constant>

    <constant name="internal error" value="-1">
        Met internal inconsistency.
    </constant>

    <constant name="decode base64 url failed" value="-101">
        Faled to decode Base64URL string.
    </constant>

    <constant name="init random failed" value="-102">
        Faled to initialize random module.
    </constant>

    <constant name="parse jwt failed" value="-201">
        Failed to parse JWT.
    </constant>

    <constant name="sign jwt failed" value="-202">
        Failed to produce JWT signature.
    </constant>
</enum>
