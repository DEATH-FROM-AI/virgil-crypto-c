<interface name="key">
    Common information about asymmetric key.

    <inherit interface="alg"/>

    <method name="key len" is_const="1">
        Length of the key in bytes.

        <return type="size"/>
    </method>


    <method name="key bitlen" is_const="1">
        Length of the key in bits.

        <return type="size"/>
    </method>
</interface>
