<interface name="key reader">
    Provide interface to for key marshaling.


    <inherit interface="error context"/>


    <method name="read public key">
        Read public key object.

        <argument name="data" class="data"/>

        <return interface="public key" access="disown"/>
    </method>


    <method name="read private key">
        Read private key object.

        <argument name="data" class="data"/>

        <return interface="private key" access="disown"/>
    </method>
</interface>
