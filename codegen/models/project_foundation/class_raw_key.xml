<class name="raw key">
    Provide implementation agnostic representation of the asymmetric key.

    <property name="alg" enum="key alg"/>
    <property name="bytes" class="buffer"/>


    <constructor name="with members">
        Creates fully defined raw key.

        <argument name="alg" enum="key alg"/>
        <argument name="bytes" class="buffer" access="retain"/>
    </constructor>


    <method name="alg">
        Returns asymmetric algorithm type that raw key belongs to.

        <return enum="key alg"/>
    </method>


    <method name="bytes">
        Return raw key bytes.

        <return class="buffer" access="retain"/>
    </method>
</class>
