<class name="ratchet chain key" context="public">

    <require class="ratchet common" scope="public"/>

    <property name="index" type="unsigned" size="4"/>
    <property name="key" type="byte" access="readwrite">
        <array length="fixed" length_constant=".(class_ratchet_common_constant_ratchet_shared_key_length)"/>
    </property>

    <method name="clone" is_const="1">
        <argument name="dst" class="self" access="readwrite"/>
    </method>

</class>
