<class name="ratchet common hidden" context="none" scope="private">
    <constant name="message version" value="1"/>
    <constant name="group message version" value="1"/>
    <constant name="session version" value="1"/>
    <constant name="group session version" value="1"/>

    <constant name="shared key len" value="32"/>
    <constant name="key len" value="32"/>

    <constant name="max skipped messages" value="200"/>
    <constant name="max skipped dh" value="5"/>
    <constant name="max message gap" value="2000"/>

    <constant name="max epochs count" value="5"/>
    <constant name="max skipped epochs count" value="4"/>

    <constant name="prekey message len" value="139"/>

    <constant name="max group regular message len" value="32918"/>
    <constant name="max group info message len" value="82"/>

    <constant name="max cipher text len" value="32768"/>

    <constant name="max group session len" value="4191503"/>
    <constant name="max session len" value="42531"/>

    <require header="pb_decode.h" scope="public"/>
    <require header="pb_encode.h" scope="public"/>

    <require class="buffer" project="common" scope="private"/>

    <method name="buffer decode callback" is_static="1">
        <argument name="stream" class="pb_istream_t" library="nanopb" access="readwrite"/>
        <argument name="field" class="pb_field_t" library="nanopb"/>
        <argument name="arg" class="void*" library="nanopb" access="readwrite"/>
        <argument name="max size" type="size"/>

        <return type="boolean"/>
    </method>

    <method name="buffer encode callback" is_static="1">
        <argument name="stream" class="pb_ostream_t" library="nanopb" access="readwrite"/>
        <argument name="field" class="pb_field_t" library="nanopb"/>
        <argument name="arg" class="any" access="readwrite">
            <array length="given" access="readonly"/>
        </argument>

        <return type="boolean"/>
    </method>
</class>