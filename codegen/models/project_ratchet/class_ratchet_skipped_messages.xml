    <require header="RatchetSession.pb.h" scope="public"/>
    <require header="pb_decode.h" scope="public"/>
    <require header="pb_encode.h" scope="public"/>

    <property name="keys" class="ratchet skipped message key list node" access="readwrite"/>

    <method name="find key">
        <argument name="counter" type="size"/>
        <argument name="ratchet public key" class="data"/>

        <return class="ratchet skipped message key" access="readwrite"/>
    </method>

    <method name="delete key">
        <argument name="skipped message key" class="ratchet skipped message key" access="readwrite"/>
    </method>

