<enum name="error" definition="public">
    <constant name="success" of_class="global" value="0">
        No errors was occurred.
    </constant>

    <constant name="asn1 write error" of_class="global" value="1">
        Error during ASN.1 serialization
    </constant>

    <constant name="asn1 read error" of_class="global" value="2">
        Error during ASN.1 deserialization
    </constant>

    <constant name="message version doesn't match" of_class="global" value="3">
        Message version doesn't match
    </constant>

    <constant name="bad message" of_class="global" value="4">
        Bad message
    </constant>

    <constant name="aes error" of_class="global" value="5">
        AES error
    </constant>

    <constant name="wrong message format" of_class="global" value="6">
        Wrong message format
    </constant>

    <constant name="invalid arguments" of_class="global" value="7">
        Invalid arguments
    </constant>

    <constant name="curve25519 error" of_class="global" value="8">
        curve25519 error
    </constant>
</enum>
