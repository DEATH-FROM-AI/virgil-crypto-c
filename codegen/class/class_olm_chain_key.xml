<class name="olm chain key" context="public">

    <require class="ratchet common" scope="public"/>

    <property name="index" type="size" size="4"/>
    <property name="key" type="byte" access="readwrite">
        <array length="fixed" length_constant=".(class_ratchet_common_constant_olm_shared_key_length)"/>
    </property>

    <method name="clone">
        <argument class="context" access="readonly"/>
        <argument name="clone" class="context"/>
    </method>

</class>
