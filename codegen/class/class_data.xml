<class name="data" context="public" lifecycle="none">
    Encapsulates fixed byte array.


    <property name="bytes" type="data">
        Underlying byte array.
    </property>


    <property name="len" type="size">
        Byte array length.
    </property>


    <method name="is valid">
        Returns true if underlying byte array is defined.

        <argument class="context" access="readonly" is_reference="0"/>
        <return type="boolean"/>
    </method>


    <method name="data">
        Creates data from the preallocated bytes.

        <argument name="bytes" type="data"/>
        <argument name="len" type="size"/>
        <return class="context" access="readonly" is_reference="0"/>
    </method>


    <method name="empty">
        Creates emty data.

        <return class="context" access="readonly" is_reference="0"/>
    </method>
</class>
